{
    "name": "mcfedr/queue-manager-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for managing job queues",
    "keywords": ["queue", "job", "schedule", "task", "beanstalkd"],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.3",
        "mcfedr/queue-driver-bundle": "~1.2"
    },
    "require-dev": {
        "mcfedr/queue-driver-pheanstalk-bundle": "~1.2"
    },
    "suggest": {
       "mcfedr/queue-driver-pheanstalk-bundle": "A driver for beanstalkd"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "mcfedr\\Queue\\QueueManagerBundle": "src/" }
    }
}
